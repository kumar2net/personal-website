{
  "buildCommand": "cd apps/personal-website && npm run build",
  "installCommand": "npm install --workspaces --legacy-peer-deps",
  "outputDirectory": "apps/personal-website/dist",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD .",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
